{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "node ./build/framework/server.js",
    "build": "tsc & npx tailwindcss -i ./public/styles.css -o ./public/styles_export.css",
    "build:watch": "tsc -w",
    "dev": "npm run build:watch & npm run start & npx tailwindcss -i ./public/styles.css -o ./public/styles_export.css --watch"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "tailwindcss": "^3.4.1"
  }
}
